<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-schema="sales" match-name=".*"/>
  <table-filter match-catalog="sales" match-name="Customers"/>
  <table-filter match-catalog="sales" match-name="OrderDetails"/>
  <table-filter match-catalog="sales" match-name="Orders"/>
  <table-filter match-catalog="sales" match-name="Products"/>
</hibernate-reverse-engineering>